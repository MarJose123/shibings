{
  "expo": {
    "name": "shibings",
    "slug": "shibings",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/launcher/ic_launcher-web.png",
    "scheme": "shibings",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/launcher/playstore-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#DAC59D"
    },
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/launcher/ic_launcher-web.png",
        "backgroundColor": "#DAC59D"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/launcher/ic_launcher-web.png\"",
      "build": {
        "babel": {
          "include": [
            "@ui-kitten/components"
          ]
        }
      }
    },
    "plugins": [
      "expo-router",
      "expo-build-properties",
      {
        "android": {
          "compileSdkVersion": 34,
          "targetSdkVersion": 34,
          "buildToolsVersion": "34.0.0",
          "minSdkVersion": 30,
          "enableProguardInReleaseBuilds": true
        }
      }
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
